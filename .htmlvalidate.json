{
  "extends": [
    "htmlvalidate:recommended"
  ],
  "plugins": [
    "./rules.js"
  ],
  "rules": {
    "attr-quotes": "off",
    "camel/relative-links": "error",
    "camel/structured-data": "error",
    "camel/title": "error",
    "doctype-style": [
      "error",
      {
        "style": "lowercase"
      }
    ],
    "element-required-attributes": "off",
    "long-title": "off",
    "no-inline-style": "off",
    "prefer-tbody": "off",
    "wcag/h30": "off"
  }
}
