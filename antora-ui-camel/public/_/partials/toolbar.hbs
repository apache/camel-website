<nav class="toolbar" aria-label="Toolbar">
{{> nav-toggle}}
  {{#with site.homeUrl}}
  <a href="{{{relativize this}}}" class="home-link{{#if @root.page.home}} is-current{{/if}}"></a>
  {{/with}}
{{> breadcrumbs}}
{{> page-versions}}
  {{#if (and page.fileUri (not env.CI))}}
  <div class="edit-this-page"><a href="{{page.fileUri}}" title="Edit this page on GitHub">{{> edit-icon}}<span class="edit-label">Edit this Page</span></a></div>
  {{else if (and page.editUrl (or env.FORCE_SHOW_EDIT_PAGE_LINK (not page.origin.private)))}}
  <div class="edit-this-page"><a href="{{editPageUrl page}}" title="Edit this page on GitHub">{{> edit-icon}}<span class="edit-label">Edit this Page</span></a></div>
  {{/if}}
</nav>
